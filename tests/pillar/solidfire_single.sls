cinder:
  volume:
    enabled: true
    version: liberty
    backend:
      solidfire:
        type_name: normal-storage
        engine: solidfire
        san_ip: 127.0.0.1
        san_login: username
        san_password: password
        clustername: cluster1
        sf_emulate_512: false
    identity:
      engine: keystone
      host: 127.0.0.1
      port: 35357
      tenant: service
      user: cinder
      password: pwd
      region: regionOne
    osapi:
        host: 127.0.0.1
    glance:
        host: 127.0.0.1
        port: 9292
